{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/cyber-cafe/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/v1/auth/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/v1/users/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/v1/blog/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/v1/admin/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api/v1/chat/(.*)",
            "dest": "server.js"
        },
        {
            "src": "/api-docs",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
} 