{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/cyber-cafe/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/auth/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/users/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/blog/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/admin/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api/v1/chat/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "dest": "/api/index.js"
        },
        {
            "src": "/api-docs(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
} 